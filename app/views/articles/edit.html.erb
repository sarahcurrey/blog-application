<h1> Edit Article </h1>

<% form_for @article, url: article_path do |f| %>
  <p>
    <%= f.label :title %> <br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :text%> <br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.submit %> <br>
  </p>

<% end %>

  <% link_to 'Back', article_path %>
