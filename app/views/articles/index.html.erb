<h1> Current Event Articles </h1>

<table>
  <tr>
    <th> Title </th>
    <th> Author </th>
    <th> Text </th>
    <th> Date </th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.author %></td>
      <td><%= article.text %></td>
      <td><%= article.created_at %></td>
      <td><%= link_to 'Show', article_path(article) %></td>
    </tr>
  <% end %>
 </table>
